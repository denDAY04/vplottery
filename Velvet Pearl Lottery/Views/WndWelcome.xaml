<Window x:Class="Velvet_Pearl_Lottery.Views.WndWelcome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Velvet_Pearl_Lottery"
        mc:Ignorable="d"
        Title="Velvet Pearl Lottery" Height="350" Width="525"
        ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight"
        Style="{StaticResource Datapad}">
    <Grid>
        <Button x:Name="BtwNewLottery" Content="New Lottery" Margin="219,146,220,155" Click="BtwNewLottery_Click" TabIndex="0"/>
        <Label x:Name="LabWelcome" Content="Welcome To The Velvet Pearl Lottery" Margin="58,72,57,0" VerticalAlignment="Top" FontSize="24" FontFamily="Palatino Linotype"/>
        <Button x:Name="BtnLoadLottery" Content="Load Lottery" Margin="219,195,220,106" Click="BtnLoadLottery_Click" TabIndex="1"/>

    </Grid>
</Window>
